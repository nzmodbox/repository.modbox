        <dir>
            <info compressed="false">https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/repo/zips/</datadir>
        </dir>
		<dir minversion="18.0" maxversion="18.9.0">
            <info compressed="false">https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/leia/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/leia/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/leia/zips/</datadir>
        </dir>
        <dir minversion="18.9.0">
            <info compressed="false">https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/matrix/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/matrix/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/nzmodbox/repository.modbox/master/matrix/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Modbox Repo</summary>
        <description>Repository for the modbox wizard</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
